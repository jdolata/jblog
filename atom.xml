<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Imię Nazwisko</title>
 <link href="http://sigma.inf.ug.edu.pl/~jdolata/atom.xml" rel="self"/>
 <link href="http://sigma.inf.ug.edu.pl/~jdolata/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://sigma.ug.edu.pl/~jdolata/</id>
 <author>
   <name>Jędrzej Dolata</name>
   <email>jdolata@sigma.inf.ug.edu.pl</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://sigma.inf.ug.edu.pl/~jdolata/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://sigma.ug.edu.pl/~jdolata/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
